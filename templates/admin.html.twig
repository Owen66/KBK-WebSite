
{% block content %}
    <h1>Create New User</h1>

    <form
            method="post"
            action="/createUser"
            >

        <p>
            Username:
            <input type="text" name="username" autofocus>
        </p>
        <p>
            Password:
            <input type="password" name="password">
        </p>
        <input type="submit" value="create">
    </form>

{% endblock %}